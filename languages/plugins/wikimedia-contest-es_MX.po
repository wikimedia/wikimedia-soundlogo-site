#: inc/gravity-forms.php:225 build/submissionForm.js:1
#: src/js/submission-form/sound-validator.js:99
msgid "File must be one of the allowed types: MP3, OGG, or WAV."
msgstr ""
"El archivo debe estar en uno de los formatos admitidos: MP3, OGG o WAV."

#: inc/gravity-forms.php:232 build/submissionForm.js:1
#: src/js/submission-form/sound-validator.js:107
msgid "File must be less than 100MB."
msgstr "Los documentos deben tener menos de 100 MB."

#: inc/gravity-forms.php:239 build/submissionForm.js:1
#: src/js/submission-form/sound-validator.js:126
msgid "Sound must be less than 10s."
msgstr "El sonido deberá durar menos de 10s."

#: inc/gravity-forms.php:244 build/submissionForm.js:1
#: src/js/submission-form/sound-validator.js:131
msgid "Sound must be at least 1s."
msgstr "El sonido debe durar al menos 1s."

#: inc/submitters.php:90
#, fuzzy
msgid "Invalid email address."
msgstr "Dirección de correo electrónico no válida."

#: inc/submitters.php:95 inc/submitters.php:235
#, fuzzy
msgid ""
"You have met the maximum number of submissions for this contest. Each "
"contestant can submit up to 3 sound logos."
msgstr ""
"Has cumplido con el número máximo de presentaciones para este concurso. Cada "
"concursante puede enviar hasta 3 logotipos de sonido."

#: build/submissionForm.js:1 src/js/submission-form/sound-validator.js:136
#, fuzzy
msgid "Sound should be less than 4s."
msgstr "El sonido debe ser inferior a 4s."

#: build/submissionForm.js:1 src/js/submission-form/sound-validator.js:147
msgid "MP3 files should be at least 192kbps."
msgstr "Los archivos MP3 deben tener al menos 192kbps."

#: build/submissionForm.js:1 src/js/submission-form/sound-validator.js:152
msgid "OGG files should be at least 160kbps."
msgstr "Los archivos OGG deben tener al menos 160kbps."

#: build/submissionForm.js:1 src/js/submission-form/sound-validator.js:175
msgid "Audio file is not readable."
msgstr "El archivo de audio no es legible."

